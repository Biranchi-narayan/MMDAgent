# Queries
[InfoQuery]
	(what *[Linking] [SpecNode] *'s [Info])
	(what *'s [SpecNode] *'s [Info])
	(i WANT [SpecNode] *'s [Info])
	(can you FIND *me [SpecNode] *'s [Info])
FIND
	(find)
	(get)
	(tell)
	(search for)
WANT
	(want)
	(need)
	(require)
	
;

[NodeQuery]
	(WHWORD [GenNode] [RelVerb] [SpecNode])
	(WHWORD [GenNode] [AllLinking] [SpecNode] [RelVerb] *[Prep])
	(WHWORD [GenNode] [AllLinking] [RelVerb] by [SpecNode])
WHWORD
	(what)
	(which)
;

[PersonQuery]
	(who [Linking] [SpecNode])
	(who [Linking] [SpecNode] *'s [GenPerson])
;

[LocationQuery]
	(where *[Linking] [SpecLocation])
	(where *[Linking] [SpecNode] 's [GenLocation])
	(where *[_do] [SpecNode] [LocationVerb])
;

